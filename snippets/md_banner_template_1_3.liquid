          </div>
          {%- assign use_css = true -%}
          {%- endunless -%}
          {%- assign image_item = block.settings.image_1 -%}
          {%-assign width = image_item.width | default: 380.00 -%}{%-assign height = image_item.height | default: 160.00 | plus: 0.00-%}
