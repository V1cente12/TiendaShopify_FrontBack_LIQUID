<!-- about-template-2.liquid -->
{{page.content|html}}
{%- for block in section.blocks -%}
	{%- if block.type == 'item_who_we_are' -%}
	{%-unless block.settings.off_block-%}

	{%- assign width_section = block.settings.width_section -%}
	{%- assign width_content = block.settings.width_content -%}
	{%- capture width_section_ -%}{%-if width_section == 'container-fluid wide' and width_content == false -%}w-100{%-else-%}{{width_section}}{%-endif-%}{%- endcapture -%}
	{%- capture width_content_ -%}{%-if width_content and width_section == 'w-100'-%}container{%-elsif width_section == 'container-fluid wide' and width_content == false -%}container-fluid wide{%-else-%}w-100{%-endif-%}
	{%- endcapture -%}
  {%-render 'sc_css', sc: block -%}
	<div class="{{width_section_}}{%if block.settings.off_mobile%} d-none d-lg-block{%endif%}{%if block.settings.off_desktop%} d-lg-none{%endif%}" data-block="item_who_we_are">
	  <div class="content_{{block.id}}" data-respon="{'el':'.content_{{block.id}}','mt':'{{block.settings.section_margin_top}}','mb':'{{block.settings.section_margin_bottom}}','p':'{{block.settings.section_padding}}'}">
      <div class="section-width {{width_content_}}">
      	<div class="row">
	      	<div class="col-lg-10 offset-lg-1 text-center">
            <h2 class="title">{{block.settings.title}}</h2>
            {{block.settings.html_content}}
            <div class="about-images mb-5">
              {%if block.settings.image_signature %}
            	{%- assign image_item = block.settings.image_signature -%}
            	<img class="lazyload" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" data-src="{% render 'kt_image_format',cache:false,src: image_item,size: 'master' %}" alt="{{ image_item.alt | default: shop.name | escape }}" style="margin: 0 auto;">
            	{% endif %}
            </div>		            
            <div class="about-images">
              {%if block.settings.image %}
            	{%- assign image_item = block.settings.image -%}
						  {%-assign width = image_item.width | default: 930.00 -%}{%-assign height = image_item.height | default: 390.00 | plus: 0.00-%}
						  <div class="img__banner lazyload" style="padding-bottom: {{height|divided_by: width | times: 100.00000000}}%;" data-bgset="{%- render 'bgset', image: image_item -%}" data-sizes="auto" aria-label="{{shop.name | escape}}"{% unless block.settings.image %} title="560x360 px"{% endunless %}></div>
            	{% endif %}
            </div>
          </div>
      	</div>
      </div>
	  </div>
	</div>
	{%-endunless-%}
	{%- elsif block.type == 'item_icons' -%}
	{%-unless block.settings.off_block-%}

	{%- assign width_section = block.settings.width_section -%}
	{%- assign width_content = block.settings.width_content -%}
	{%- capture width_section_ -%}{%-if width_section == 'container-fluid wide' and width_content == false -%}w-100{%-else-%}{{width_section}}{%-endif-%}{%- endcapture -%}
	{%- capture width_content_ -%}{%-if width_content and width_section == 'w-100'-%}container{%-elsif width_section == 'container-fluid wide' and width_content == false -%}container-fluid wide{%-else-%}w-100{%-endif-%}
	{%- endcapture -%}
  {%-render 'sc_css', sc: block -%}
	<div class="{{width_section_}}{%if block.settings.off_mobile%} d-none d-lg-block{%endif%}{%if block.settings.off_desktop%} d-lg-none{%endif%}" data-block="item_icons">
		<div class="content_{{block.id}}" data-respon="{'el':'.content_{{block.id}}','mt':'{{block.settings.section_margin_top}}','mb':'{{block.settings.section_margin_bottom}}','p':'{{block.settings.section_padding}}'}">
      <div class="section-width {{width_content_}}">
				{%- assign count = 0 -%}
				{%- if block.settings.title_1 != blank -%}
					{% assign count = count | plus: 1 %}
				{%- endif -%}
				{%- if block.settings.title_2 != blank -%}
					{% assign count = count | plus: 1 %}
				{%- endif -%}
				{%- if block.settings.title_3 != blank -%}
					{% assign count = count | plus: 1 %}
				{%- endif -%}
				{%- if block.settings.title_4 != blank -%}
					{% assign count = count | plus: 1 %}
				{%- endif -%}
				{%-case count-%}
					{%- when 0-%}
						{%- assign item_grid = 'col-12' -%}
					{%- when 2-%}
						{%- assign item_grid = 'col-12 col-sm-6' -%}
					{%- when 3-%}
						{%- assign item_grid = 'col-12 col-sm-6 col-md-4' -%}
					{%- when 4-%}
						{%- assign item_grid = 'col-12 col-sm-6 col-md-3' -%}
				{%-endcase-%}
				<div class="row">
        	{%- for i in (1..4) -%}
        	{%- assign title = 'title_' | append: i -%}
        	{%- if block.settings[title] != blank -%}
        	{%- assign content = 'content_' | append: i -%}
        	{%- assign icon_font = 'icon_font_' | append: i -%}
        	{%- assign icon = 'icon_' | append: i -%}
        	{%- assign url = 'url_' | append: i -%}
        	<div class="{{item_grid}}">
	        	<div class="icon-box icon-box-sm text-center">
	            <span class="icon-box-icon">
	              {%- if block.settings[icon_font] -%}
	              <a class="text-primary" href="{{block.settings[url]|default:'#'}}">
	                <i class="{{block.settings[icon_font] | default: 'fkt-puzzle-piece'}}"></i>
	              </a>
	              {%- else -%}
	              <a href="{{block.settings[url]|default:'#'}}">
	              	{%- assign image_item = block.settings[icon] -%}
		            	<img class="lazyload" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" data-src="{% render 'kt_image_format',cache:false,src: image_item,size: 'master' %}" alt="{{ image_item.alt | default: shop.name | escape }}" style="margin: 0 auto;">
	              </a>
	              {%- endif -%}
	            </span>
	            <div class="icon-box-content">
	              <h3 class="icon-box-title">{{block.settings[title]}}</h3>
	              <p>{{block.settings[content]}}</p>
	            </div>
	          </div>
	        </div>
        	{%- endif -%}
        	{%- endfor -%}
				</div>
      </div>
	  </div>
	</div>
	{%-endunless-%}
	{%- elsif block.type == 'item_count' -%}
	{%-unless block.settings.off_block-%}

	{%- assign width_section = block.settings.width_section -%}
	{%- assign width_content = block.settings.width_content -%}
	{%- capture width_section_ -%}{%-if width_section == 'container-fluid wide' and width_content == false -%}w-100{%-else-%}{{width_section}}{%-endif-%}{%- endcapture -%}
	{%- capture width_content_ -%}{%-if width_content and width_section == 'w-100'-%}container{%-elsif width_section == 'container-fluid wide' and width_content == false -%}container-fluid wide{%-else-%}w-100{%-endif-%}
	{%- endcapture -%}
  {%-render 'sc_css', sc: block -%}
	<div class="{{width_section_}}{%if block.settings.off_mobile%} d-none d-lg-block{%endif%}{%if block.settings.off_desktop%} d-lg-none{%endif%}" data-block="item_count" data-section-id="{{ block.id }}" id="Countto-{{ block.id }}" data-section-type="countto-section">
		<div class="content_{{block.id}}{%if block.settings.image%} bg-image lazyload{%endif%}"{%if block.settings.image%} data-bgset="{{block.settings.image | img_url: 'master'}}" data-sizes="auto" data-parent-fit="contain"{%endif%} data-respon="{'el':'.content_{{block.id}}','mt':'{{block.settings.section_margin_top}}','mb':'{{block.settings.section_margin_bottom}}','p':'{{block.settings.section_padding}}'}">
      <div class="section-width {{width_content_}}">
				{%- assign count = 0 -%}
				{%- if block.settings.title_1 != blank -%}
					{% assign count = count | plus: 1 %}
				{%- endif -%}
				{%- if block.settings.title_2 != blank -%}
					{% assign count = count | plus: 1 %}
				{%- endif -%}
				{%- if block.settings.title_3 != blank -%}
					{% assign count = count | plus: 1 %}
				{%- endif -%}
				{%- if block.settings.title_4 != blank -%}
					{% assign count = count | plus: 1 %}
				{%- endif -%}
				{%-case count-%}
					{%- when 0-%}
						{%- assign item_grid = 'col-12' -%}
					{%- when 2-%}
						{%- assign item_grid = 'col-6' -%}
					{%- when 3-%}
						{%- assign item_grid = 'col-6 col-md-4' -%}
					{%- when 4-%}
						{%- assign item_grid = 'col-6 col-md-3' -%}
				{%-endcase-%}
				<div class="row">
        	{%- for i in (1..4) -%}
        	{%- assign title = 'title_' | append: i -%}
        	{%- if block.settings[title] != blank -%}
        	{%- assign count = 'count_' | append: i -%}
        	{%- assign unit = 'unit_' | append: i -%}
        	<div class="{{item_grid}}">
        		<div class="count-container text-center">
							<div class="count-wrapper text-white">
								<span class="count kt_countTo" data-from="0" data-to="{{block.settings[count]}}" data-speed="3000" data-refresh-interval="50">{{block.settings[count]}}</span>{{block.settings[unit]}}
							</div>
							<h3 class="count-title text-white">{{block.settings[title]}}</h3>
						</div>
	        </div>
        	{%- endif -%}
        	{%- endfor -%}
				</div>
      </div>
	  </div>
	</div>
	{%-endunless-%}
	{%- elsif block.type == 'item_meet_our_team' -%}
	{%-unless block.settings.off_block-%}

	{%- assign width_section = block.settings.width_section -%}
	{%- assign width_content = block.settings.width_content -%}
	{%- capture width_section_ -%}{%-if width_section == 'container-fluid wide' and width_content == false -%}w-100{%-else-%}{{width_section}}{%-endif-%}{%- endcapture -%}
	{%- capture width_content_ -%}{%-if width_content and width_section == 'w-100'-%}container{%-elsif width_section == 'container-fluid wide' and width_content == false -%}container-fluid wide{%-else-%}w-100{%-endif-%}
	{%- endcapture -%}
  {%-render 'sc_css', sc: block -%}
	<div class="{{width_section_}}{%if block.settings.off_mobile%} d-none d-lg-block{%endif%}{%if block.settings.off_desktop%} d-lg-none{%endif%}" data-block="item_meet_our_team">
		<div class="content_{{block.id}}" data-respon="{'el':'.content_{{block.id}}','mt':'{{block.settings.section_margin_top}}','mb':'{{block.settings.section_margin_bottom}}','p':'{{block.settings.section_padding}}'}">
      <div class="section-width {{width_content_}}">
      	<h2 class="title text-center mb-4">{{block.settings.title}}</h2>
      	<div class="row justify-content-center">
        	{%- for i in (1..9) -%}
        	{%- assign image = 'image_avatar_' | append: i -%}
        	{%- if block.settings[image] != blank -%}
        	{%- assign name = 'name_' | append: i -%}
        	{%- assign info = 'info_' | append: i -%}
        	{%- assign social_twitter_link = 'social_twitter_link_' | append: i -%}
        	{%- assign social_facebook_link = 'social_facebook_link_' | append: i -%}
        	{%- assign social_instagram_link = 'social_instagram_link_' | append: i -%}
          <div class="col-md-3">
            <div class="member member-2 text-center">
              <figure class="member-media">
            	{%- assign image_item = block.settings[image] -%}
						  {%-assign width = image_item.width -%}{%-assign height = image_item.height | plus: 0.00-%}
						  <div class="img__banner lazyload" style="padding-bottom: {{height|divided_by: width | times: 100.00000000}}%;" data-bgset="{%- render 'bgset', image: image_item -%}" data-sizes="auto" aria-label="{{shop.name | escape}}"></div>
                <figcaption class="member-overlay">
                  <div class="member-overlay-content">
                    <div class="social-icons social-icons-simple">
                    	{%- if block.settings[social_twitter_link] != blank -%}
                      <a href="{{block.settings[social_twitter_link]}}" class="social-icon" title="Facebook" target="_blank"><i class="fkt-facebook-f"></i></a>
				              {%- endif -%}
                    	{%- if block.settings[social_facebook_link] != blank -%}
                      <a href="{{block.settings[social_twitter_link]}}" class="social-icon" title="Twitter" target="_blank"><i class="fkt-twitter"></i></a>
				              {%- endif -%}
                    	{%- if block.settings[social_instagram_link] != blank -%}
                      <a href="{{block.settings[social_twitter_link]}}" class="social-icon" title="Instagram" target="_blank"><i class="fkt-instagram"></i></a>
				              {%- endif -%}
                    </div>
                  </div>
                </figcaption>
              </figure>
              <div class="member-content">
                <h3 class="member-title">{{block.settings[name]}}</h3>
              </div>
            </div>
          </div>
          {%- endif -%}
          {%- endfor -%}
        	<div class="col-12 mt-3 text-center">
        		<a href="{{block.settings.btn_url|default:'#'}}" class="btn btn-sm btn-minwidth-lg {{block.settings.style_btn}} {{block.settings.style_btn_rounded}}{%if block.settings.btn_shadow%} btn-shadow{%endif%}" aria-label="{{block.settings.name_btn| strip_html}}">{{block.settings.name_btn}}</a>
        	</div>
        </div>
      </div>
		</div>
	</div>
	{%-endunless-%}
	{%- elsif block.type == 'item_brands_info' -%}
	{%-unless block.settings.off_block-%}

	{%- assign width_section = block.settings.width_section -%}
	{%- assign width_content = block.settings.width_content -%}
	{%- capture width_section_ -%}{%-if width_section == 'container-fluid wide' and width_content == false -%}w-100{%-else-%}{{width_section}}{%-endif-%}{%- endcapture -%}
	{%- capture width_content_ -%}{%-if width_content and width_section == 'w-100'-%}container{%-elsif width_section == 'container-fluid wide' and width_content == false -%}container-fluid wide{%-else-%}w-100{%-endif-%}
	{%- endcapture -%}
  {%-render 'sc_css', sc: block -%}
	<div class="{{width_section_}}{%if block.settings.off_mobile%} d-none d-lg-block{%endif%}{%if block.settings.off_desktop%} d-lg-none{%endif%}" data-block="item_brands_info">
		<div class="content_{{block.id}}" data-respon="{'el':'.content_{{block.id}}','mt':'{{block.settings.section_margin_top}}','mb':'{{block.settings.section_margin_bottom}}','p':'{{block.settings.section_padding}}'}">
      <div class="section-width {{width_content_}}">
      	<div class="row text-center">
          <div class="col-lg-10 offset-lg-1">
            <div class="brands-text mb-4 mx-auto">
              <h2 class="title">{{block.settings.title}}</h2>
              <p>{{block.settings.html_content}}</p>
            </div>
          </div>
          <div class="col-lg-10 offset-lg-1">
            <div class="brands-display">
              <div class="row">
              	{%- for i in (1..9) -%}
              	{%- assign image = 'image_' | append: i -%}
              	{%- assign brand_call_to_action = 'brand_call_to_action_' | append: i -%}
              	{%- if block.settings[image] != blank -%}
                <div class="image__brand col-sm-3">
                  <a href="{{ block.settings[brand_call_to_action] | default: '#' }}" class="brand">
                  	{%- assign image_item = block.settings[image] -%}
	                  {%-assign width = image_item.width -%}{%-assign height = image_item.height | plus: 0.00-%}
	                  <div class="img__banner position-relative lazyload none-bg" style="padding-bottom: {{height|divided_by: width | times: 100.00000000}}%;background-size: auto;" data-bgset="{%- render 'bgset', image: image_item -%}" data-sizes="auto"></div>
                  </a>
                </div>
              	{%- endif -%}
              	{%- endfor -%}
              </div>
            </div>
          </div>
        </div>
      </div>
		</div>
	</div>
	{%-endunless-%}
	{%- endif -%}
{%- endfor -%}
<style>
	/* Counters */
	.count-container {
		margin-bottom: 3.3rem;
	}

	.count-wrapper {
		color: #333;
		font-weight: 400;
		font-size: 3rem;
		line-height: 1.2;
		margin-bottom: .6rem;
	}

	.count-title {
		font-weight: 400;
		font-size: 1.6rem;
		letter-spacing: -.01em;
	}
	.member {
	  margin-bottom: 2rem;
	  max-width: 376px;
	  margin-left: auto;
	  margin-right: auto;
	}
	.member p {
	  max-width: 240px;
	}
	.member.text-center p {
	  margin-left: auto;
	  margin-right: auto;
	}
	.member.text-center .social-icons {
	  justify-content: center;
	}

	.member-media {
	  position: relative;
	  margin: 0;
	}

	.member-content {
	  padding-top: 2.4rem;
	  padding-bottom: 2.4rem;
	  overflow: hidden;
	}

	.member-title {
	  font-weight: 400;
	  font-size: 1.6rem;
	  letter-spacing: 0;
	  margin-bottom: 0;
	}
	.member-title span {
	  display: block;
	  color: #999999;
	  font-weight: 300;
	  font-size: 1.4rem;
	  margin-top: 0.3rem;
	}

	.member-overlay {
	  position: absolute;
	  left: 0;
	  right: 0;
	  top: 0;
	  bottom: 0;
	  display: flex;
	  align-items: center;
	  justify-content: center;
	  background-color: rgba(102, 102, 102, 0.7);
	  color: #fff;
	  opacity: 0;
	  visibility: hidden;
	  overflow: hidden;
	}
	.member-overlay .member-title {
	  color: inherit;
	  margin-bottom: 1.5rem;
	}
	.member-overlay .member-title span {
	  color: #ebebeb;
	}
	.member-overlay .social-icons {
	  margin-top: 2.7rem;
	}
	.member-overlay .social-icon {
	  font-size: 1.5rem;
	}
	.member-overlay .social-icon + .social-icon {
	  margin-left: 2.8rem;
	}
	.member-overlay .social-icon:not(:hover) {
	  color: inherit;
	}

	.member-content,
	.member-overlay {
	  transition: all 0.45s ease;
	}

	.member:hover .member-content {
	  opacity: 0;
	}
	.member:hover .member-overlay {
	  visibility: visible;
	  opacity: 1;
	}

	.member.member-2 {
	  background-color: #fff;
	  max-width: 277px;
	  margin-left: auto;
	  margin-right: auto;
	}
	.member.member-2 .member-content {
	  visibility: visible;
	  opacity: 1;
	  padding-top: 1.7rem;
	  padding-bottom: 2.7rem;
	}
	.member.member-2 .member-overlay {
	  color: #333;
	  background-color: rgba(249, 249, 249, 0.7);
	}
	.member.member-2 .member-overlay .social-icons {
	  margin-top: 0;
	  margin-bottom: 0;
	}
	.brands-text {
    max-width: 430px;
    margin-top: .5rem;
	}
</style>
{%schema%}
  {
    "name": "About page",
    "settings": [
      {
        "type": "header",
        "content": "Content settings"
      }
    ],
    "blocks": [
      {
        "type": "item_who_we_are",
        "name": "Item Who We Are",
        "settings": [
		      {
		        "type":"checkbox",
		        "id": "off_block",
		        "label": "Turn off block?"
		      },
		      {
		        "type":"checkbox",
		        "id": "off_mobile",
		        "label": "Turn off on mobile?"
		      },
		      {
		        "type":"checkbox",
		        "id": "off_desktop",
		        "label": "Turn off on desktop?"
		      },
		      {
		        "type": "header",
		        "content": "Space settings"
		      },
		      {
		        "type": "radio",
		        "id": "width_section",
		        "label": "Max width section?",
		        "options": [
		          { "value": "container", "label": "1200px" },
		          { "value": "container-fluid wide", "label": "Wide" },
		          { "value": "w-100", "label": "Full width" }
		        ],
		        "default": "container"
		      },
		      {
		        "type":"checkbox",
		        "id": "width_content",
		        "label": "Max width content?",
		        "default": true
		      },
		      {
		        "type": "text",
		        "id": "section_margin_top",
		        "label": "Margin top",
		        "info": "2.5rem|3rem|3.5rem|4rem|4.5rem|5rem"
		      },
		      {
		        "type": "text",
		        "id": "section_margin_bottom",
		        "label": "Margin bottom",
		        "info": "2.5rem|3rem|3.5rem|4rem|4.5rem|5rem"
		      },
		      {
		        "type": "text",
		        "id": "section_padding",
		        "label": "Paddings (top right bottom left)",
		        "info": "6rem 6rem 0 6rem"
		      },
		      {
		        "type": "header",
		        "content": "Background settings"
		      },
		      {
		        "type": "color",
		        "id": "section_bg",
		        "label": "Background color"
		      },
		      {
		        "type": "range",
		        "id": "section_bg_op",
		        "min": 0,
		        "max": 1,
		        "step": 0.1,
		        "label": "Opacity background color",
		        "default": 1
		      },
		      {
		        "type": "header",
		        "content": "Content settings"
		      },
		      {
		      	"type": "text",
		      	"id": "title",
		      	"label": "Title",
		      	"default": "Who We Are"
		      },
		      {
		        "type": "html",
		        "id": "html_content",
		        "label": "Text content",
		        "default": "<p>Sed pretium, ligula sollicitudin laoreet viverra, tortor libero sodales leo, eget blandit nunc tortor eu nibh. Suspendisse potenti. Sed egestas, ante et vulputate volutpat, uctus metus libero eu augue. Morbi purus libero, faucibus adipiscing, commodo quis, gravida id, est. Sed lectus. Praesent elementum hendrerit tortor. Sed semper lorem at felis.<\/p>"
		      },
		      {
		        "type": "image_picker",
		        "id": "image_signature",
		        "label": "Signature image",
		        "info": "Size suggestion: 140 x 46 pixels"
		      },
		      {
		        "type": "image_picker",
		        "id": "image",
		        "label": "Banner image",
		        "info": "Size suggestion: 930 x 390 pixels"
		      }
        ]
      },
      {
        "type": "item_icons",
        "name": "Item Icon box",
        "settings": [
		      {
		        "type":"checkbox",
		        "id": "off_block",
		        "label": "Turn off block?"
		      },
		      {
		        "type":"checkbox",
		        "id": "off_mobile",
		        "label": "Turn off on mobile?"
		      },
		      {
		        "type":"checkbox",
		        "id": "off_desktop",
		        "label": "Turn off on desktop?"
		      },
		      {
		        "type": "header",
		        "content": "Space settings"
		      },
		      {
		        "type": "radio",
		        "id": "width_section",
