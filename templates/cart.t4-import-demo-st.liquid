<script>  
  if (T4.demo_crr !== undefined && T4.demo_crr.current.content_for_index.length == 20) {
    $('.t4-import-demo-st-content .alert-warning').removeClass('d-none');
    $('.t4-import-demo-st-content .item-import').addClass('loading');
  }
  if (!theme.csrfToken) {
    $.ajax({
      type: "GET",
      url: "/admin/themes/"+Shopify.theme.id,
      success: function (data){
        theme.csrfToken = data.split('name="csrf-token"')[1].split(" />")[0].split('"')[1];
		    $('.item-import').removeClass('loading');
      }
    });
  } else {
    $('.item-import').removeClass('loading');
  }
  $('.t4-import-dm-i-st .item-import').on('click', function (event) {
    event.preventDefault();
    var $this = $(this);
    var url = $this.attr('data-full-url');
    var pos = $this.attr('data-pos');
    $('.t4-import-demo-st-content .item-import').addClass('loading');
    $.getJSON( url, function(data) {
      T4.demo = data;
      $('.item-import').removeClass('loading');
      $('.t4-import-demo-st-popup').addClass('on-preview');
      $(pos).show();
    })
  });
  $(document).on('click', '.t4-import-sts .btn-st-import .btn', function (event) {
    event.preventDefault();
    var $this = $(this);
    $this.addClass('loading');
    var id = $this.attr('data-id');
    var new_st = T4.demo.current.sections[id];
    var block_new_st = {};
    var block_order_new_st = [];
    var id_st = Date.now().toString();
    var idx = 0;
    $.each(T4.demo.current.sections[id].blocks, function(index, val) {
      block_order_new_st.push(id_st +'-'+ idx);
      block_new_st[id_st +'-'+ idx] = val;
      idx++;
    });
    new_st.blocks = block_new_st;
    new_st.block_order = block_order_new_st;
    T4.demo_crr.current.sections[id_st] = new_st;
    T4.demo_crr.current.content_for_index.push(id_st);
    $.ajax({
      url:  '/admin/api/2021-07/themes/'+Shopify.theme.id+'/assets.json',
      headers: {
        "content-type": "application/x-www-form-urlencoded; charset=UTF-8",
        "x-csrf-token": theme.csrfToken
      },
      type: 'PUT',
      data: {asset:  { 'key': 'config/settings_data.json', "value": JSON.stringify(T4.demo_crr)}},
    })
    .done(function() {
      $this.removeClass('loading');
      window.top.location.reload();
    });
  });
  $(document).on('click', '.t4-import-demo-st-btn-close', function(event){
    event.preventDefault();
    $('.t4-import-demo-st').removeClass('open-import-demo-st');
  });
  $(document).on('click', '.t4-import-demo-st-close-preview', function(event){
    event.preventDefault();
    $('.t4-import-demo-st-popup').removeClass('on-preview');
    $('.t4-import-sts').hide();
  });
</script>