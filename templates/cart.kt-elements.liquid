  .import-form-fields {
    margin-top: 15px;
    font-weight: 600;
  }
  .buttons {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    width: 100%;
    display: flex;
    justify-content: center;
    z-index: 1;
  }
  .buttons button{
    cursor: pointer;
    padding: 12px 20px;
    background-color: #a62c2b;
    color: #fff;
    border: none;
    outline: none;
    font-weight: bold;
    opacity: 0;
    transform: translateY(20px);
    -webkit-transition: all 0.3s ease;
    -moz-transition: all 0.3s ease;
    -o-transition: all 0.3s ease;
    transition: all 0.3s ease;
  }
  .item-import:hover .buttons button{
    opacity: 1;
    transform: translateY(0);
  }
  .import.--demo {
    background-color: #333;
    border-radius: 50px 0 0 50px;
  }
  .import.--section {
    border-radius: 0 50px 50px 0;
  }
  .--thumb {
    position: relative;
    overflow: hidden;
  }
  .item-import.loading{
    pointer-events: none;
  }
  .item-import.loading .--thumb:before {
    content: '';
    position: absolute;
    top: -20%;
    left: -20%;
    right: -20%;
    bottom: -20%;
    background-image: linear-gradient(90deg,rgba(255,255,255,0) 20%,rgba(255,255,255,.8) 50%,rgba(255,255,255,0) 80%);
    animation: placeloading 1s infinite;
