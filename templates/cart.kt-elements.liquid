<script>
  "undefined" == typeof window.T4 && (window.T4 = {});
  "undefined" == typeof window.theme && (window.theme = {});
  T4.grid = '';
  $.getScript( "//cdn.shopify.com/s/files/1/0279/6568/5813/t/2/assets/isotope-plus.pkgd.min.js?v=13908884544623468046" )
    .done(function( script, textStatus ) {
      T4.grid = $('.els .imports').isotope({
          itemSelector: '.els .import-dm-i',
          filter: '.import-dm-i.--demo',
          layoutMode: 'packery'
        });
    })
    .fail(function( jqxhr, settings, exception ) {
  });
  $('.list-elements .filter_i').on('click', function (event) {
    event.preventDefault();
    var filter_ = $(this).attr('filter');
    T4.grid.isotope({ filter: '.import-dm-i' + filter_ });
    $('.els .content').animate({scrollTop:0},800);
  });
  if (!theme.csrfToken) {
    $.ajax({
      type: "GET",
      url: "/admin/themes/"+Shopify.theme.id,
      success: function (data){
        theme.csrfToken = data.split('name="csrf-token"')[1].split(" />")[0].split('"')[1];
		    $('.item-import').removeClass('loading');
      }
    });
  } else {
    $('.item-import').removeClass('loading');
  }
  $('.import.--demo').on('click', function (event) {
    event.preventDefault();
    var $this = $(this);
    $('.item-import').addClass('loading');
    $.ajax({
      url:  '/admin/api/2021-07/themes/'+Shopify.theme.id+'/assets.json',
      headers: {
        "content-type": "application/x-www-form-urlencoded; charset=UTF-8",
        "x-csrf-token": theme.csrfToken
      },
      type: 'PUT',
      data: {asset:  { 'key': 'config/settings_data.json', 'source_key': $this.attr('data-js')}},
    })
    .done(function() {
	  	$('.item-import').removeClass('loading');
	    window.top.location.reload();
    });
  });
  $('.import.--section').on('click', function (event) {
    event.preventDefault();
    var $this = $(this);
    var url = $this.attr('data-full-url');
    var json = $this.attr('data-json');
    "undefined" == typeof T4.demo && (T4.demo = []);
    T4.grid.isotope({ filter: '.import-dm-i.--' + json.replace('Demo_','') });
    $('.els .content').animate({scrollTop:0},800);
    if (T4.demo[json]) {return}
    $('.item-import').addClass('loading');
    $.getJSON( url, function(data) {
      T4.demo[json] = data;
    });
    $.getJSON( '/admin/api/2021-07/themes/'+Shopify.theme.id+'/assets?asset[key]=config/settings_data.json', function(data) {
      T4.demo_crr = JSON.parse(data.asset.value);
      $('.item-import').removeClass('loading');
    })
  });
  $('.import.--section_i').on('click', function (event) {
    event.preventDefault();
    var $this = $(this);
    $this.addClass('loading');
    var id = $this.attr('data-id');
    var json = $this.attr('data-json');
    var new_st = T4.demo[json].current.sections[id];
    var block_new_st = {};
    var block_order_new_st = [];
    var id_st = Date.now().toString();
    var idx = 0;
    $.each(T4.demo[json].current.sections[id].blocks, function(index, val) {
      block_order_new_st.push(id_st +'-'+ idx);
      block_new_st[id_st +'-'+ idx] = val;
      idx++;
    });
    new_st.blocks = block_new_st;
    new_st.block_order = block_order_new_st;
    T4.demo_crr.current.sections[id_st] = new_st;
    T4.demo_crr.current.content_for_index.push(id_st);
    $.ajax({
